# Development Environment - Local Docker Setup
# Copy this file to the project root as .env.development
# These values are used when running: npm run dev

# PeerJS Server Configuration
VITE_PEERJS_HOST=localhost
VITE_PEERJS_PORT=9000
VITE_PEERJS_PATH=/peerjs
VITE_PEERJS_SECURE=false

# STUN/TURN Server Configuration
VITE_STUN_URL=stun:localhost:3478
VITE_TURN_URLS=turn:localhost:3478
VITE_TURN_USERNAME=webrtc
VITE_TURN_CREDENTIAL=webrtc123
