# Production Environment - Self-Hosted Server
# Copy this file to the project root as .env.production
# Update values with your production server details

# PeerJS Server Configuration
# Replace 'your-domain.com' with your actual domain or server IP
VITE_PEERJS_HOST=your-domain.com
VITE_PEERJS_PORT=9000
VITE_PEERJS_PATH=/peerjs
VITE_PEERJS_SECURE=true

# STUN/TURN Server Configuration
# Replace 'your-domain.com' with your actual domain or server IP
VITE_STUN_URL=stun:your-domain.com:3478
VITE_TURN_URLS=turn:your-domain.com:3478,turns:your-domain.com:5349
VITE_TURN_USERNAME=webrtc
VITE_TURN_CREDENTIAL=webrtc123
